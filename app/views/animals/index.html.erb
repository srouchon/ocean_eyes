<h1>Animals#index</h1>
<p>Find me in app/views/animals/index.html.erb</p>

<ul>
  <% @animals.each do |animal| %>
    <li><%= link_to animal.common_name, animal_path(animal) %></li>
    <h2><%= animal.common_name %></h2>
    <h5><%= animal.latin_name %></h5>
    <li><%= animal.description %></li>
    <br>
  <% end %>
</ul>
