<div class='ar-container d-flex'>
  <div class='ar-bg'>
    <a-scene embedded arjs='trackingMethod: best;'>
      <a-assets>
        <a-asset-item id="model" src="ar/BarramundiFish.gltf" crossOrigin="anonymous"></a-asset-item>
      </a-assets>

      <a-gltf-model src="#model" position="0 0.5 0" scale="0.05 0.05 0.05"></a-gltf-model>
      <a-marker-camera preset='hiro'></a-marker-camera>
      <a-camera-static/>
    </a-scene>
  </div>
  <div class='ar-quit-button'>
    <%#= link_to "Back", root_path %>
    <!-- <a href="/" onclick="window.location.reload(true);">Back</a>  </div> -->
    <a data-turbolinks='false' href="/"><i class="fas fa-arrow-circle-left"></i></a>
    <%#= redirect_to @root_path, turbolinks: false %>
</div>


